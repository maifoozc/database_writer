# MQTT broker (required on private server)
MQTT_BROKER_HOST=serverIP
MQTT_BROKER_PORT=1883
MQTT_TOPIC=ems/site/+

# Optional auth
# MQTT_USERNAME=
# MQTT_PASSWORD=
# MQTT_USE_TLS=0
# MQTT_CLIENT_ID=mqtt_to_db

# PostgreSQL (required)
DATABASE_URL=postgresql://postgres:postgres@host:5432/ems_db

# Optional
# DB_BATCH_SIZE=500
